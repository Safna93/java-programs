import java.util.*;



public class ListDemo
{
	public static void main(String[] args) 
		
	
	{
	ArrayList list=new ArrayList();
	list.add("Safna");
	list.add("Anu");
	list.add("Niit");
	list.add(new Integer(10));
        
/*System.out.println(list);//displaying

String str=(String)list.get(2);//retrieving
System.out.println("Element in 2nd index is"+ str);

list.remove(2);//remove
System.out.println("After removing" +list);*/
//Enhanced for loop:it is used for traversing the elements in collection

for(Object ob:list)
{
   if(ob instanceof String)
           {
            String str=(String)ob;
            System.out.println("String is:"+str);
	}
else if(ob instanceof Integer)
{
Integer intr=(Integer)ob;
System.out.println("Integer is:"+intr);
}

 

	}
}
}