import java.util.Scanner;
public class Palindrome
{
public static void main(String[] args)
{
	int i,j,sum=0;
	Scanner sc=new Scanner(System.in);
	System.out.println("Enter the number");
	j=sc.nextInt();
	
	while(j>10)
	{
		i=j%10;
		sum=(sum*10)+i;
		j=j/10;
	}
		System.out.println("The palindrome is"+sum);	
}
}
